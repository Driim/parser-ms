{
  "name": "parser-microservice",
  "version": "0.1.0",
  "description": "SwatcherBot Parser Microservices",
  "author": "Dmitriy Falko <dmitriy.falko@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --fix '{src,apps,libs}/**/*.ts'",
    "test": "jest --verbose true --runInBand",
    "test:watch": "jest --watch --verbose true --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.10.14",
    "@nestjs/microservices": "^6.11.8",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/schedule": "^0.4.0",
    "@types/cheerio": "^0.22.18",
    "@types/lodash.uniqwith": "^4.5.6",
    "@types/mongoose": "^5.7.7",
    "@types/tough-cookie": "^4.0.0",
    "axios": "^0.19.2",
    "axios-cookiejar-support": "^1.0.0",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "0.11.0",
    "escape-string-regexp": "^2.0.0",
    "feedparser-promised": "^2.0.1",
    "lodash.uniqwith": "^4.5.0",
    "mongodb": "^3.5.4",
    "mongoose": "^5.9.5",
    "mongoose-autopopulate": "^0.12.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rss-parser": "^3.8.0",
    "rxjs": "^6.5.4",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^6.10.14",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
